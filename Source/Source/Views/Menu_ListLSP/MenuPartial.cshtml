                <a href="@Url.Action("Index","GioiThieu")">Giới Thiệu</a>
            </li>
            <img src="~/Assets/images/topbar-separator.png">
            <li>
                <button data-toggle="modal" data-target="#cart" onclick="XemGioHang()" ><img src="~/Assets/images/cart.png"> Giỏ Hàng</button>
            </li>
        </ul>
    </div>
</div>
<script>
    function XemGioHang() {
        debugger
        $.ajax({
            url: '/GioHang/GioHang_ChiTiet'
